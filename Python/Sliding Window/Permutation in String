class Solution:
    def checkInclusion(self, s1: str, s2: str) -> bool:
        left = 0
        right = len(s1)
        c1 = Counter(s1)
    
        while right <= len(s2):
            c2 = Counter(s2[left:right])
            if c2 == c1:
                return True

            right += 1
            left += 1
        return False            
