class Solution {
    public int rob(int[] nums) {
        int rob = 0;
        int norob = 0;
        for (int i = 0; i < nums.length; i ++) {
            int nextRob = norob + nums[i];
            int nextNorob = Math.max(rob, norob);
            rob = nextRob;
            norob = nextNorob;
        }
        return Math.max(rob, norob);
    }
}
